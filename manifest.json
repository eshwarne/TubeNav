{
    "manifest_version" : 2,
    "name" : "TubeNav",
    "version":"1.0.0",
    "description":"Search and watch videos, short films on the fly!",

    "icon":{
        "128" : "128.png",
        "48" : "48.png",
        "16" : "16.png"
    },

    "background":{
        "scripts" : ["eventPage.js"],
        "persistent" : false
    },
    "permissions" : [
        "tabs", 
        "contextMenus"
    ]
}